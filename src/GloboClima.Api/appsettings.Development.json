{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Secret": "your-super-secret-key-for-development-only-change-in-production-minimum-32-characters",
    "Issuer": "GloboClima",
    "Audience": "GloboClima",
    "ExpirationHours": 24
  },
  "OpenWeatherMap": {
    "ApiKey": "YOUR_OPENWEATHERMAP_API_KEY_HERE"
  },
  "WeatherAPI": {
    "ApiKey": "YOUR_WEATHERAPI_KEY_HERE"
  },
  "AWS": {
    "Profile": "default",
    "Region": "us-east-1"
  },
  "DynamoDB": {
    "ServiceURL": "http://localhost:8000",
    "UseLocalDb": true
  }
}
